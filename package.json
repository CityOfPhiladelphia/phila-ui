    "babel-eslint": "^10.0.3",
    "conventional-changelog-conventionalcommits": "^4.2.1",
    "cssnano": "^4.1.10",
    "eslint": "^6.5.1",
    "eslint-plugin-vue": "^5.2.3",
    "rollup": "^1.23.1",
    "rollup-plugin-alias": "^2.0.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
